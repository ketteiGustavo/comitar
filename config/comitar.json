{
  "default_push": false,
  "ask_push": true,
  "use_commit_body": false,
  "use_colors": true,
  "use_blinks": true,
  "use_emojis": true,
  "use_scope": false,
  "add_all_files": true,
  "control_tag": false,
  "version_control": false,
  "version_targets": [],
  "version_build": false,
  "generate_changelog": true,
  "changelog_path": "CHANGELOG.md",
  "default_types": [
    {
      "type": "feat",
      "emoji": "✨",
      "label": "Novo recurso"
    },
    {
      "type": "fix",
      "emoji": "🐛",
      "label": "Correção de bug"
    },
    {
      "type": "docs",
      "emoji": "📚",
      "label": "Documentação"
    },
    {
      "type": "test",
      "emoji": "🧪",
      "label": "Testes"
    },
    {
      "type": "build",
      "emoji": "📦",
      "label": "Build / dependências"
    },
    {
      "type": "perf",
      "emoji": "⚡",
      "label": "Performance"
    },
    {
      "type": "style",
      "emoji": "👌",
      "label": "Estilo / revisão"
    },
    {
      "type": "refactor",
      "emoji": "♻️",
      "label": "Refatoração"
    },
    {
      "type": "chore",
      "emoji": "🔧",
      "label": "Configuração / tarefas"
    },
    {
      "type": "ci",
      "emoji": "🧱",
      "label": "Integração contínua"
    },
    {
      "type": "raw",
      "emoji": "🗃️",
      "label": "Dados RAW"
    },
    {
      "type": "cleanup",
      "emoji": "🧹",
      "label": "Limpeza de código"
    },
    {
      "type": "remove",
      "emoji": "🗑️",
      "label": "Remoção de arquivos"
    }
  ],
  "custom_types": [
    {
      "type": "accessibility",
      "emoji": "♿",
      "label": "Acessibilidade"
    },
    {
      "type": "responsive",
      "emoji": "📱",
      "label": "Responsividade"
    },
    {
      "type": "animation",
      "emoji": "💫",
      "label": "Animações / Transições"
    },
    {
      "type": "seo",
      "emoji": "🔍️",
      "label": "SEO"
