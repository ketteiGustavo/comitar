    },
    {
      "type": "text",
      "emoji": "📝",
      "label": "Texto / Conteúdo"
    },
    {
      "type": "typing",
      "emoji": "🏷️",
      "label": "Tipagem"
    },
    {
      "type": "error",
      "emoji": "🥅",
      "label": "Tratamento de erros"
    },
    {
      "type": "soon",
      "emoji": "🔜",
      "label": "Lista de ideias"
    }
  ]
